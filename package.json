{
  "name": "quantum-defi-protocol",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "Quantum-Resistant Cross-Chain DeFi Protocol with AI-Powered Yield Optimization",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "hardhat test",
    "test:unit": "hardhat test test/ConfidentialYieldProtocol.test.js",
    "test:basic": "npx mocha test/basic.test.js --timeout 10000",
    "test:frontend": "jest test/fhe-client.test.js test/ConfidentialYieldInterface.test.js",
    "test:integration": "hardhat test test/ConfidentialYieldProtocol.test.js --grep 'Integration Tests'",
    "test:security": "hardhat test test/ConfidentialYieldProtocol.test.js --grep 'Privacy|Security'",
    "test:performance": "hardhat test test/ConfidentialYieldProtocol.test.js --grep 'Performance'",
    "test:all": "npm run test:unit && npm run test:frontend && npm run test:integration",
    "test:run": "node scripts/run-tests.js",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js --network localhost",
    "deploy:localhost": "hardhat run scripts/deploy.js --network localhost",
    "deploy:testnet": "hardhat run scripts/deploy-testnet-real-assets.js --network sepolia",
    "deploy:mumbai": "hardhat run scripts/deploy-testnet-real-assets.js --network mumbai",
    "deploy:arbitrum": "hardhat run scripts/deploy-testnet-real-assets.js --network arbitrumSepolia",
    "deploy:base": "hardhat run scripts/deploy-testnet-real-assets.js --network baseSepolia",
    "deploy:linea": "hardhat run scripts/deploy-linea.js --network linea",
    "deploy:linea-sepolia": "hardhat run scripts/deploy-linea-sepolia.js --network lineaSepolia",
    "deploy:all-testnets": "npm run deploy:testnet && npm run deploy:mumbai && npm run deploy:arbitrum && npm run deploy:base && npm run deploy:linea-sepolia",
    "deploy:all-mainnets": "npm run deploy:linea",
    "deploy:fhe": "hardhat run scripts/deploy-with-fhe.js --network sepolia",
    "verify:sepolia": "hardhat verify --network sepolia",
    "verify:mumbai": "hardhat verify --network mumbai",
    "verify:arbitrum": "hardhat verify --network arbitrumSepolia",
    "verify:base": "hardhat verify --network baseSepolia",
    "verify:linea": "hardhat verify --network linea",
    "verify:linea-sepolia": "hardhat verify --network lineaSepolia",
    "node": "hardhat node"
  },
  "keywords": [
    "defi",
    "cryptocurrency",
    "blockchain",
    "quantum-resistant",
    "cross-chain",
    "ai",
    "yield-optimization",
    "web3",
    "ethereum",
    "polygon",
    "arbitrum"
  ],
  "author": "Quantum DeFi Labs",
  "license": "MIT",
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@solana/spl-token": "^0.4.1",
    "@solana/web3.js": "^1.87.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "10.4.16",
    "dotenv": "16.3.1",
    "ethers": "^5.8.0",
    "next": "14.0.4",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.6"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-verify": "^2.1.1",
    "@openzeppelin/contracts": "^4.9.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "chai": "4.3.7",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.4",
    "hardhat": "^2.26.3",
    "hardhat-gas-reporter": "1.0.9",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha": "^10.2.0",
    "typescript": "5.2.2"
  }
}
